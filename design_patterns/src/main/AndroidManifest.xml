<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.hadlink.design_patterns"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".CommonActivity"/>
        <activity android:name=".status.LoginActivity"/>
        <activity android:name=".status.StateMainAty"/>
        <activity android:name=".prototype.PrototypeMainAty"/>
        <activity android:name=".visitor.VisitorMainAty"/>
        <activity android:name=".proxy.ProxyMainAty"/>
        <activity android:name=".observer.ObserMainAty"/>
    </application>

</manifest>
